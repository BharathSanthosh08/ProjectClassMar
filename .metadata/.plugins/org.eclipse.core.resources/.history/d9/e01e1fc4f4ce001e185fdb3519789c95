package MechBlock;

public  class Child  {
	
	public static void main(String[] args) {

		int mark =50 ;
		String name ="bharath" ;
		if (mark>=20) {
			if (name=="Bharath") {
			System.out.println("Pass");
			
			}
			else {
				System.out.println("fail");
			}
				}
				
		

		else {
			System.out.println("not matched");
		}
			

}			
				
				
				
				
	}				
				
				
		
