package MechBlock;

public class Class2 {
	
	public static void main(String[] args) {
		int even =0;
		int odd =0;
		int evensum=0;
		int oddsum=0;
		for (int i=0;i<=100;i++) {
			if(i%2==0) {
				System.out.println("even"+i);
				
				even++;
				evensum=evensum+i;
			}
			else {
			System.out.println("odd"+i);
			odd++;
			oddsum=oddsum+i;
			
		}
		
		
	}
		System.out.println(even);
		System.out.println(odd); 

	}}
